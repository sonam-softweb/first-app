<center>
<font size=3><b>Create a New Lender Application</b></font><BR><BR>
<%= error_messages_for 'lender' %>
<% form_for :lender, :html => {:multipart => true} do |a| %>
  <Table Border=0>
  <TR>
  <TD Align=right>
  First Name:
  </TD>
  <TD Align=left>
  <%= a.text_field :first_name %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Last Name:
  </TD>
  <TD Align=left>
  <%= a.text_field :last_name %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  E-Mail Address:
  </TD>
  <TD Align=left>
  <%= a.text_field :email %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Screen Name:
  </TD>
  <TD Align=left>
  <%= a.text_field :screen_name %>
  </TD>
  </TR>


  <TR>
  <TD Align=right>
  Street Address:
  </TD>
  <TD Align=left>
  <%= a.text_field :street_address %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  City:
  </TD>
  <TD Align=left>
  <%= a.text_field :city %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  State:
  </TD>
  <TD Align=left>
  <%= a.text_field :state %>
  </TD>
  </TR>


  <TR>
  <TD Align=right>
  Phone:
  </TD>
  <TD Align=left>
  <%= a.text_field :phone %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  SS Number:
  </TD>
  <TD Align=left>
  <%= a.text_field :ss %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Bank Account:
  </TD>
  <TD Align=left>
  <%= a.text_field :bank_account %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Bank Routing:
  </TD>
  <TD Align=left>
  <%= a.text_field :bank_routing%>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Our Notes:
  </TD>
  <TD Align=left>
  <%= a.text_area :our_notes, :maxlength => 400, :size => "30x5"  %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Bank Balance:
  </TD>
  <TD Align=left>
  <%= a.text_field :bank_balance %>
  </TD>
  </TR>

<!--
  <TR>
  <TD Align=right>
  Social Security Number:
  </TD>
  <TD Align=left>
  <%= a.text_field(:ss, :size=>8) %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Credit Card Number:
  </TD>
  <TD Align=left>
  <%= a.text_field :cc_number %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Credit Card Expiration Date:
  </TD>
  <TD Align=left>
  <%= a.text_field :cc_expiration %>
  </TD>
  </TR>

  <TR>
  <TD Align=right>
  Credit Card CID:
  </TD>
  <TD Align=left>
  <%= a.text_field :cc_cid %>
  </TD>
  </TR>




